

{   
    "version": 2,
    "builds": [
        {
            "src" : "build_files.sh",
            "use" : "@vercel/static-build",
            "config" : {
            "disDir" : "staticfiles_build"
            }
        }
    ],
    "routes": [
        {
            "src" : "/static/(.*)",
            "dest" : "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/crashdjango/wsgi.py"
        }
    ]
}
